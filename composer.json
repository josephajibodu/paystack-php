{
    "name": "josephajibodu/paystack-php",
    "description": "Modern Paystack SDK with PHP",
    "keywords": [
        "payments",
        "paystack",
        "php",
        "modern"
    ],
    "homepage": "https://github.com/josephajibodu/paystack-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph Ajibodu",
            "email": "josephajibodu@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.20",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "JosephAjibodu\\Paystack\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JosephAjibodu\\Paystack\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
